<!DOCTYPE html>
<html>
<head>
    <title>Images</title>
</head>
<body>
    <h1>Images from the Database</h1>
    <% images.forEach(image => { %>
        <div>
            <h3><%= image.titulo %></h3>
            <p><%= image.descripcion %></p>
            <img src="/images/<%= image.ruta %>" alt="<%= image.titulo %>">
        </div>
    <% }); %>
</body>
</html>


